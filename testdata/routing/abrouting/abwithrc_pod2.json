{
    "apiVersion": "v1",
    "kind": "List",
    "items": [
        {
            "apiVersion": "v1",
            "kind": "ReplicationController",
            "metadata": {
                "labels": {
                    "name": "test-rc"
                },
                "name": "test-rc-2"
            },
            "spec": {
                "replicas": 1,
                "template": {
                    "metadata": {
                        "labels": {
                            "name": "test-pods-2",
			    "type": "test2"
                        }
                    },
                    "spec": {
                        "containers": [
                            {
                                "image": "aosqe/caddy-docker-2",
                                "name": "test-pods-2"
                            }
                        ]
                    }
                }
            }
        },
        {
            "apiVersion": "v1",
            "kind": "Service",
            "metadata": {
                "labels": {
                    "name": "service-secure-2"
                },
                "name": "service-secure-2"
            },
            "spec": {
                "ports": [
                    {
                        "name": "https",
                        "port": 27443,
                        "protocol": "TCP",
                        "targetPort": 8443
                    }
                ],
                "selector": {
                    "type": "test2"
                }
            }
        },
       {
            "apiVersion": "v1",
            "kind": "Service",
            "metadata": {
                "labels": {
                    "name": "service-unsecure-2"
                },
                "name": "service-unsecure-2"
            },
            "spec": {
                "ports": [
                    {
                        "name": "http",
                        "port": 27017,
                        "protocol": "TCP",
                        "targetPort": 8080
                    }
                ],
                "selector": {
                    "type": "test2"
                }
            }
        }

    ]
}
